<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd//hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.asso.model.Doc" table="doc" catalog="asso">
        <id name="docid" type="java.lang.Integer">
            <column name="docid" />
            <generator class="identity" />
        </id>
        <property name="createdate" type="java.lang.String">
            <column name="createdate" length="20" />
        </property>        
        <property name="step" type="int">
            <column name="step" length="11" />
        </property>
        <property name="userid" type="int">
            <column name="userid" length="11" />
        </property>
        <property name="formid" type="int">
            <column name="formid" length="11" />
        </property>
        
    </class>
</hibernate-mapping>
