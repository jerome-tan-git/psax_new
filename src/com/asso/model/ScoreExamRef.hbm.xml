<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd//hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.asso.model.ScoreExamRef" table="scorexamref" catalog="asso">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="chosenrefid" type="int">
            <column name="chosenrefid" length="11" />
        </property>
        <property name="itemid" type="int">
            <column name="itemid" length="11" />
        </property>
        <property name="scoreid" type="int">
            <column name="scoreid" length="11" />
        </property>
    </class>
</hibernate-mapping>
