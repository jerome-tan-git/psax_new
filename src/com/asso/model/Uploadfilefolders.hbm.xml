<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd//hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.asso.model.Uploadfilefolders" table="uploadfilefolders" catalog="asso">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="foldername" type="java.lang.String">
            <column name="foldername" length="500" />
        </property>
         <property name="auther" type="java.lang.String">
            <column name="auther" length="100" />
        </property>
        <property name="autherid" type="int">
            <column name="autherid" length="11" />
        </property> 
        <property name="createtime" type="java.lang.String">
            <column name="createtime" length="20" />
        </property>        
    </class>
</hibernate-mapping>
